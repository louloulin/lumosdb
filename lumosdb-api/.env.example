# 服务器配置
PORT=3001
NODE_ENV=development

# CORS 配置
CORS_ORIGIN=http://localhost:3000

# 日志配置
LOG_LEVEL=info

# SQLite 配置
SQLITE_DB_PATH=./data/sqlite

# DuckDB 配置
DUCKDB_DB_PATH=./data/duckdb.db

# 向量数据库配置
VECTOR_DB_TYPE=milvus
VECTOR_DB_URL=http://localhost:19530
VECTOR_DB_USERNAME=
VECTOR_DB_PASSWORD=

# AI 配置
AI_PROVIDER=openai
AI_API_KEY=your_openai_api_key
AI_MODEL=gpt-4

# JWT 认证
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=24h 